{
  "name": "user-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn build:clean && tsc",
    "build:clean": "rm -rf dist",
    "dev": "ts-node src/index.ts",
    "start": "ts-node src/index.ts",
    "postinstall": "cd ../../../ && npx husky install ./server-code/services/template/.husky",
    "test:setup": "npx dotenv-cli -o -e .env.test -- npx prisma db push",
    "test": "yarn test:setup && npx dotenv-cli -o -e .env.test -- jest --config jest.config.ts"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.17.17",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "chai": "^4.3.6",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.0",
    "prisma": "^5.3.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "winston": "^3.8.1",
    "zod": "^3.22.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "lint-staged": {
    "*": [
      "npx prettier --write --ignore-unknown"
    ]
  }
}
